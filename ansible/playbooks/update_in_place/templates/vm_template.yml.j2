---
kind: Service
apiVersion: v1
metadata:
  name: {{ topic }}-{{ uuid }}{{ item }}
  namespace: vms-demo
  labels:
    type: {{ topic }}
spec:
  ports:
    - name: cockpit
      protocol: TCP
      port: 9090
      targetPort: 9090
  selector:
    kubevirt.io/domain: {{ topic }}-{{ uuid }}{{ item }}
